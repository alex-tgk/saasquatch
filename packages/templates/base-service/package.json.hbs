{
  "name": "@{{project.name}}/{{service.name}}",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "{{service.description}}",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "fastify": "^4.24.0",
    "fastify-plugin": "^5.0.1",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@sinclair/typebox": "^0.32.5",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/compress": "^6.5.0",
    "@fastify/env": "^4.3.0",
    "@fastify/swagger": "^8.13.0",
    "@fastify/swagger-ui": "^2.1.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "get-port": "^7.0.0"{{#if service.features.database}},
    "knex": "^3.1.0"{{#if (eq infrastructure.database.type "postgresql")}},
    "pg": "^8.11.3"{{/if}}{{#if (eq infrastructure.database.type "sqlite")}},
    "better-sqlite3": "^9.2.2"{{/if}}{{/if}}{{#if service.features.cache}},
    "ioredis": "^5.3.2"{{/if}}{{#if service.features.messageQueue}},
    "nats": "^2.19.0"{{/if}}{{#if service.features.jwt}},
    "@fastify/jwt": "^8.0.0",
    "bcrypt": "^5.1.1"{{/if}}
  },
  "devDependencies": {
    "@types/node": "^20.10.0"{{#if service.features.jwt}},
    "@types/bcrypt": "^6.0.0"{{/if}},
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "supertest": "^7.1.4",
    "@types/supertest": "^6.0.3"
  }
}
